{
  "name": "gcp-pubsub-exploration",
  "version": "1.0.0",
  "description": "GCP Pub/Sub exploration with TypeScript producer and consumer",
  "main": "dist/index.js",
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc",
    "start:producer": "node dist/producer.js",
    "start:consumer": "node dist/consumer.js",
    "dev:producer": "ts-node src/producer.ts",
    "dev:consumer": "ts-node src/consumer.ts",
    "dev:push-server": "ts-node src/push-server.ts",
    "stats": "ts-node src/subscription-stats.ts",
    "send": "curl -X POST -H 'Content-Type: application/json' -d '{\"message\":\"Hello from pnpm send!\"}' http://localhost:3000/publish",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.0.7",
    "express": "^4.18.2",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.0"
  },
  "keywords": ["gcp", "pubsub", "typescript", "google-cloud"],
  "author": "ccortezia",
  "license": "MIT"
}